local function updateNoClipBtn()
	noclipBtn.Text = noClipActive and "NoClip: Açık" or "NoClip: Kapalı"
	noclipBtn.BackgroundColor3 = noClipActive and Color3.fromRGB(0,200,80) or Color3.fromRGB(220,50,50)
end
