local function createSlot(index)
	local card = Instance.new("Frame")
	card.Size = UDim2.new(0,140,0,140)
	card.BackgroundColor3 = Color3.fromRGB(32,32,32)
	card.Parent = SlotsHolder
	local uc = Instance.new("UICorner", card); uc.CornerRadius = UDim.new(0,12)
	local stroke = Instance.new("UIStroke", card); stroke.Thickness=1; stroke.Color=Color3.fromRGB(70,70,70)

	local lbl = Instance.new("TextLabel", card)
	lbl.Size = UDim2.new(1,0,0,28)
	lbl.Position = UDim2.new(0,0,0,6)
	lbl.BackgroundTransparency = 1
	lbl.Text = tostring(index)
	lbl.TextColor3 = Color3.fromRGB(255,255,255)
	lbl.Font = Enum.Font.GothamBold
	lbl.TextScaled = true

	-- DURUM LABEL
	local statusLbl = Instance.new("TextLabel", card)
	statusLbl.Size = UDim2.new(1, -8, 0, 24)
	statusLbl.Position = UDim2.new(0,4,0,36)
	statusLbl.BackgroundTransparency = 1
	statusLbl.Text = "İşinlanacak yer yok"
	statusLbl.TextColor3 = Color3.fromRGB(255,100,100)
	statusLbl.Font = Enum.Font.GothamSemibold
	statusLbl.TextScaled = true

	local saveBtn = Instance.new("TextButton", card)
	saveBtn.Size = UDim2.new(0.46,0,0,36)
	saveBtn.Position = UDim2.new(0.05,0,1,-42)
	saveBtn.BackgroundColor3 = Color3.fromRGB(0,170,255)
	saveBtn.Text = "Kaydet"
	saveBtn.TextColor3 = Color3.fromRGB(255,255,255)
	local sc = Instance.new("UICorner", saveBtn); sc.CornerRadius = UDim.new(0,10)

	local goBtn = Instance.new("TextButton", card)
	goBtn.Size = UDim2.new(0.46,0,0,36)
	goBtn.Position = UDim2.new(0.49,0,1,-42)
	goBtn.BackgroundColor3 = Color3.fromRGB(0,200,120)
	goBtn.Text = "Git"
	goBtn.TextColor3 = Color3.fromRGB(0,0,0)
	local gc = Instance.new("UICorner", goBtn); gc.CornerRadius = UDim.new(0,10)

	-- Kaydet davranışı
	saveBtn.MouseButton1Click:Connect(function()
		tpStore[index] = getHRP().CFrame
		statusLbl.Text = "İşinlanma hazır"
		statusLbl.TextColor3 = Color3.fromRGB(0,200,120)
		if stroke then stroke.Color = Color3.fromRGB(0,200,120) end
	end)

	-- Git davranışı
	goBtn.MouseButton1Click:Connect(function()
		if tpStore[index] then
			getHRP().CFrame = tpStore[index]
		else
			statusLbl.Text = "İşinlanacak yer yok"
			statusLbl.TextColor3 = Color3.fromRGB(255,100,100)
		end
	end)
end

for i=1,3 do createSlot(i) end

